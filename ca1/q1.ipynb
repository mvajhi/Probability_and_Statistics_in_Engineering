   "source": [
    "def create_samples(m : int, n : int, p : float):\n",
    "    samples = np.reshape(np.random.choice([0, 1], n*m, p=[1 - p, p]), (m, n))\n",
    "    return np.sum(samples, axis=1)"
   ]
